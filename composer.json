{
    "name": "jahudka/porm",
    "type": "library",
    "license": "MIT",
    "description": "Yet another PHP ORM",
    "require": {
        "php": ">=8.2",
        "nette/tokenizer": "^3.1",
        "ext-pdo_firebird": "*",
        "ext-zend-opcache": "*"
    },
    "require-dev": {
        "nette/di": "^3.2",
        "tracy/tracy": "^2.10",
        "nette/robot-loader": "^4.0",
        "symfony/console": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "PORM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "example/"
        }
    }
}
